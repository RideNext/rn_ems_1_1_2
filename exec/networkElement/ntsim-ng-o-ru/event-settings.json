{
    "nts-network-function:network-function": {
      "fault-generation": {
        "fault-delay-list": [
          {
            "index": 0,
            "delay-period": 5
          },
          {
            "index": 1,
            "delay-period": 10
          }
        ]
      },
      "ves": {
        "pnf-registration": true,
        "faults-enabled": true,
        "heartbeat-period": 0
      },
      "netconf": {
        "faults-enabled": true,
        "call-home":false
      },
      "mount-point-addressing-method": "docker-mapping",
      "function-type": "NTS_FUNCTION_TYPE_O_RAN_O_RU_FH"
    }
  }
